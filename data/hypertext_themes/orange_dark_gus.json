{
	"prefixes":{
		"link":"{{{link_icon}}}",
		"link +1":"",
		"link :inline":" ",
		"parser_error":"[PARSER_ERROR] ",
		"link_without_uri":"[PARSER MISTAKE] Link without uri: ",
		"search_without_uri":"[PARSER MISTAKE] Search without uri: "
	},
	"tag_themes":{
		"link":{
			"scale":1.1,
			"font":"italic",
			"foreground":"#D7D1C4"
		},
		"link +1":{
			"scale":1.2,
			"paragraph_background":"#111111",
			"foreground":"#F57900",
			"indent":10
		},
		"link :hover":{
			"underline":"single"
		},
		"link :prefix":{
			"scale":1.15,
			"font":"bold",
			"foreground":"#A1A49E"
		},
		"link_icon":{
			"scale":1.5
		},
		"title +0":{
			"scale":1.7,
			"foreground":"#CE5C00"
		},
		"title +1":{
			"scale":1.5
		},
		"title":{
			"scale":1.2
		},
		"quote":{
			"font":"oblique",
			"foreground":"#A1A49E"
		},
		"list_item":{
			"scale":0.9,
			"font":"italic",
			"paragraph_background":"#111111",
			"foreground":"#A1A49E",
			"indent":20
		},
		"paragraph :preformatted":{
			"wrap_mode":"none",
			"indent":10
		},
		"error":{
			"foreground":"#FB3934"
		},
		"parser_error":{
			"foreground":"#FB3934",
			"font":"italic"
		},
		"*:preformatted":{
			"font":"monospace",
			"paragraph_background":"#141414",
			"foreground":"#D3D7CF"
		},
		"*":{
			"foreground":"#D3D7CF",
			"paragraph_background":"#191919",
			"font":"Noto Sans Mono Medium 12",
			"wrap_mode":"word_char"
		}
	}
}
